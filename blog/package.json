		"eslint": "^9.14.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"globals": "^15.12.0",
		"mdsvex": "^0.12.3",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.8",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-slug": "^6.0.0",
		"sass": "^1.80.7",
		"svelte": "^5.2.7",
		"svelte-check": "^4.0.5",
		"svelte-icons": "^2.1.0",
		"svelte-preprocess": "^6.0.3",
		"tslib": "^2.8.1",
